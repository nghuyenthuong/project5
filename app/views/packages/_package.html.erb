<div id="<%= dom_id package %>">
  <p>
    <strong>Package name:</strong>
    <%= package.package_name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= package.description %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= package.price %>
  </p>

  <p>
    <strong>Duration:</strong>
    <%= package.duration %>
  </p>

  <p>
    <strong>Inclusions:</strong>
    <%= package.inclusions %>
  </p>

</div>
